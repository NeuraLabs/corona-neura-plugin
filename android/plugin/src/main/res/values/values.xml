    <style name="neura_sdk_Authentication_Button_Style" parent="@android:style/Widget.Button">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/neura_sdk_login_button_text_size</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/neura_sdk_authentication_field_text_padding</item>
    </style>
    <style name="neura_sdk_auth_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">@dimen/neura_sdk_login_text_size</item>
    </style>
    <style name="neura_sdk_auth_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/neura_sdk_light_gray_fill_selected</item>
        <item name="android:paddingTop">@dimen/neura_sdk_login_screen_padding</item>
        <item name="android:paddingBottom">@dimen/neura_sdk_login_screen_padding_small</item>
        <item name="android:paddingLeft">@dimen/neura_sdk_login_screen_padding</item>
        <item name="android:paddingRight">@dimen/neura_sdk_login_screen_padding</item>
    </style>
    <style name="neura_sdk_auth_link">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/neura_sdk_blue_btn</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">@dimen/neura_sdk_login_text_size</item>
        <item name="android:padding">@dimen/neura_sdk_padding_login</item>
    </style>
    <style name="neura_sdk_auth_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/neura_sdk_login_text_size</item>
    </style>
    <style name="neura_sdk_auth_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/neura_sdk_login_text_margin</item>
        <item name="android:layout_marginTop">@dimen/neura_sdk_login_text_margin</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/neura_sdk_title_text_size</item>
    </style>
    <style name="neura_sdk_back_button_style" parent="@android:style/Widget.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">
            @drawable/neura_sdk_authentication_signing_bottom_button_selector
